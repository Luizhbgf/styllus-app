{
  "name": "styllus-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:android": "npm run build:mobile && cd android && ./gradlew assembleRelease",
    "build:ios": "npm run build:mobile && cd ios && xcodebuild -workspace App.xcworkspace -scheme App -configuration Release archive",
    "build:mobile": "npm run build && npm run export && npx cap sync",
